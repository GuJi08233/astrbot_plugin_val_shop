{
    "monitor_time": {
        "description": "商店监控时间",
        "type": "string",
        "hint": "设置每日自动监控的时间，格式为HH:MM，如08:01",
        "default": "08:01"
    },
    "timezone": {
        "description": "时区设置",
        "type": "string",
        "hint": "设置定时任务使用的时区，如Asia/Shanghai、UTC等",
        "default": "Asia/Shanghai"
    },
    "bot_id": {
        "description": "机器人ID",
        "type": "string",
        "hint": "设置机器人的ID，用于定时任务发送消息",
        "default": "default"
    },
    "login_callback_url": {
        "description": "登录回调业务地址(s_url)",
        "type": "string",
        "hint": "默认http://connect.qq.com（与HAR成功链路一致）；可按需改为其他域名",
        "default": "http://connect.qq.com"
    },
    "login_u1_url": {
        "description": "二维码轮询回调地址(u1)",
        "type": "string",
        "hint": "默认http://connect.qq.com；建议与已验证成功的登录链路保持一致",
        "default": "http://connect.qq.com"
    }
}
